 <nav class="navbar navbar-expand-lg navbar-dark navbar-custom fixed-top">
    <div class="container">

      <a class="navbar-brand" id="logo" href="#">FOODtube</a>

      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive"
        aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
    </div>
  </nav>


  <header class="masthead text-white text-center">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-xl-9 mx-auto">
          <h2 class="mb-5">Search for your favorite recipes and learn how to make delicious meals in no time!</h1>
          <h1 class="mb-5"></h1>
        </div>
        <div class="col-md-10 col-lg-8 col-xl-7 mx-auto">
          <form id="search-term" class="form-inline" action="#resultsAnchor">

            <input id="query" class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search"
              required>

            <button class="btn btn-outline-light" id="button" type="submit"> <strong>Search</strong></button>
          </form>
        </div>
      </div>
    </div>
  </header>

  <!--Displays recipe info and id's are still movie themed-->
  <div class="container">
    <div class="row">

      <!-- container for recipe cards -->
      <div class="col-md-9" id="recipeCards">

      </div>


      <!--List of Youtube Results-->
      <div class="col-md-3" id="videos">
        <div class="card h-100 bg-light mb-3" style="max-width: 100%;">
          <div class="card-header">Videos</div>
          <div class="card-body">
            <h5 class="card-title" id="video-title"></h5>
            <p class="card-text" id="video-info"></p>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Footer -->
  <footer class="py-2 fixed-bottom" id="footer">
      <p class="m-0 text-center text-white">Copyright &copy; Project X 2018</p>
  </footer>


</body>

</html>




